{
  "name": "@pineapple-bin/prettier-plugin-miniprogram",
  "version": "1.0.3",
  "description": "prettier plugin for wechat miniprogram",
  "keywords": [
    "prettier",
    "miniprogram",
    "wechat"
  ],
  "author": "pineapplebin <pineapplewing@126.com>",
  "license": "MIT",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "env.d.ts"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/pineapplebin/prettier-plugin-miniprogram.git"
  },
  "scripts": {
    "test": "vitest",
    "build": "tsup",
    "release": "pnpm run build && bumpp"
  },
  "dependencies": {
    "@wxml/parser": "^0.4.0"
  },
  "devDependencies": {
    "@jaskang/config": "2.0.3",
    "@jaskang/tsconfig": "1.0.2",
    "@types/node": "^22.13.1",
    "bumpp": "^10.0.3",
    "prettier": "^3.5.0",
    "tsup": "^8.3.6",
    "typescript": "^5.7.3",
    "vitest": "^2.0.5"
  },
  "peerDependencies": {
    "prettier": ">=3.0.0"
  },
  "packageManager": "pnpm@9.4.0"
}
